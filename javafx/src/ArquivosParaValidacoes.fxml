<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ArquivosParaValidacoes">
   <children>
      <Label layoutX="467.0" layoutY="203.0" text="Arquivos para Validações">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ImageView fx:id="imageDocCFoto" fitHeight="150.0" fitWidth="200.0" layoutX="400.0" layoutY="285.0" pickOnBounds="true" preserveRatio="true" />
